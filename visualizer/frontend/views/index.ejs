<!DOCTYPE html>
<html>

<head>
	<meta name="viewport" content="width=device-width" />
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" />
	<link rel="stylesheet" href="../css/index.css" />
	<title>
		<%= subject %>
	</title>
	
</head>

<body>
	
	<canvas id="3DWindow" width="500px" height="500px"></canvas>
	<script >
		if (!(HTMLScriptElement.supports && HTMLScriptElement.supports('importmap'))) {
			console.log("Your browser does not support importmaps, please try another browser");
			throw new Error("Please run the application on a different browser");
		}else{
			console.log("Your browser supports importmaps");
		}
		
		const importMap = {
			imports: {
				three: './js/lib/three/build/three.module.js'
			}
		};
		const im = document.createElement('script');
		im.type = 'importmap';
		im.textContent = JSON.stringify(importMap);
		document.currentScript.after(im);
	</script>

	<script type="module" src="../js/index.js"></script>
</body>
</html>